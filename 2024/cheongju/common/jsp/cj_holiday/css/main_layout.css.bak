.menu_sub {position:relative; padding-top:60px;}
/* .menu_sub:after {display:block; position:absolute; left:0; right:0; top:50px; width:52px; height:1px; margin-right:auto; margin-left:auto; background-color:rgba(255, 255, 255, 0.2); content:'';} */
.menu_sub .nav {position:relative; width:1200px; height:269px; margin-top:55px; margin-right:auto; margin-left:auto; padding-top:52px; box-sizing:border-box;}
.menu_sub .nav:before, .menu_sub .nav:after {display:block; position:absolute; width:329px; height:146px; background-repeat:no-repeat; background-position:left top; content:'';}
.menu_sub .nav:before {top:0; left:0; background-image:url(/common/jsp/cj_holiday/images/main/icon_bg_left.png);}
.menu_sub .nav:after {bottom:0; right:0; background-image:url(/common/jsp/cj_holiday/images/main/icon_bg_right.png);}
.nav .tap_menu {width:90%; margin-right:auto; margin-left:auto;}
.nav .tap_menu li {position:relative; float:left; width:16.66%; text-align:center; box-sizing:border-box;}
.nav .tap_menu li a {position:relative; display:block; width:130px; margin-right:auto; margin-left:auto;}
.nav .tap_menu li a:after {display:block; position:absolute; top:42px; left:-15.6px; width:176px; height:50px; background:url('/common/jsp/cj_holiday/images/main/item_deco.png') no-repeat center; content:''; opacity:0; transition:0.5s;transition-timing-function:linear;}
.nav .tap_menu li a:hover:after{opacity:1; transition:1s;}

.nav .tap_menu li a span {display:block; transition:all 300ms ease-in-out;}
.nav .tap_menu li a span.img_area {position:relative; width:130px; height:130px; border:5px solid #fff08c; border-radius:100%; background-color:#fff; background-repeat:no-repeat; background-position:center; box-sizing:border-box; box-shadow:0 5px 20px rgba(255, 255, 255, 0);  transition-timing-function:linear;}
.nav .tap_menu li a:hover span.img_area {border:65px solid #fff08c; box-shadow:0 5px 20px rgba(255, 255, 255, 0.75);}


.nav .tap_menu li a:before {display:block; z-index:30; position:absolute; top:2px; left:0; width:130px; height:130px; background-repeat:no-repeat; background-position:center; content:''; opacity:0; transition-timing-function:linear;}
.nav .tap_menu li a:hover:before {opacity:1; transition:1s;}

.nav .tap_menu li.tap1 a:before{background-image:url('/common/jsp/cj_holiday/images/main/item1_hover.png');}
.nav .tap_menu li.tap2 a:before{background-image:url('/common/jsp/cj_holiday/images/main/item2_hover.png');}
.nav .tap_menu li.tap3 a:before{background-image:url('/common/jsp/cj_holiday/images/main/item3_hover.png');}
.nav .tap_menu li.tap4 a:before{background-image:url('/common/jsp/cj_holiday/images/main/item4_hover.png');}
.nav .tap_menu li.tap5 a:before{background-image:url('/common/jsp/cj_holiday/images/main/item5_hover.png');}
.nav .tap_menu li.tap6 a:before{background-image:url('/common/jsp/cj_holiday/images/main/item6_hover.png');}

.nav .tap_menu li.tap1 a span.img_area{background-image:url('/common/jsp/cj_holiday/images/main/item1.png');}
.nav .tap_menu li.tap2 a span.img_area{background-image:url('/common/jsp/cj_holiday/images/main/item2.png');}
.nav .tap_menu li.tap3 a span.img_area{background-image:url('/common/jsp/cj_holiday/images/main/item3.png');}
.nav .tap_menu li.tap4 a span.img_area{background-image:url('/common/jsp/cj_holiday/images/main/item4.png');}
.nav .tap_menu li.tap5 a span.img_area{background-image:url('/common/jsp/cj_holiday/images/main/item5.png');}
.nav .tap_menu li.tap6 a span.img_area{background-image:url('/common/jsp/cj_holiday/images/main/item6.png');}

.nav .tap_menu li a span.text {margin-top:15px; color:#fff; font-size:18px; line-height:24px; letter-spacing:-0.025em;}
.nav .tap_menu li:nth-child(3) a span.text {letter-spacing:-0.1em;}
.nav .tap_menu li a:hover span.text {color:#6e767a;}

.web_move .menu_sub .nav .tap_menu {position:relative;}
.web_move .menu_sub .nav:before {display:block; position:fixed; width:100%; height:11px; background:url(/common/jsp/cj_holiday/images/main/visual_bg_top.png) no-repeat center top; content:'';}

.web_move .menu_sub .nav:before, .web_move .menu_sub .nav:after {display:none;}
.web_move .nav .tap_menu li a:before, .web_move .nav .tap_menu li a:after {display:none;}
.web_move .nav {position:fixed; z-index:100; left:0; top:-55px; width:100%; height:140px; padding:0; background:url('/common/jsp/cj_holiday/images/main/visual_bg_m_v2.jpg') no-repeat center top; background-size:cover;}
.web_move .nav ul {width:900px; margin:90px auto 0;}
.web_move .nav .tap_menu li {display:table; width:16.66%; height:50px; color:#fff;}
.web_move .nav .tap_menu li a {display:table-cell; width:100%; vertical-align:middle;}
.web_move .nav .tap_menu li a:hover, .web_move .nav .tap_menu li a:focus, .web_move .nav .tap_menu li a:checked, .web_move .nav .tap_menu li.on a {height:50px; border-top-left-radius:30px; border-top-right-radius:30px;}
.web_move .nav .tap_menu li.tap1 a:hover, .web_move .nav .tap_menu li.tap1 a:focus, .web_move .nav .tap_menu li.tap1 a:checked, .web_move .nav .tap_menu li.tap1.on a{background-color:#68bbc0; color:#fff;}
.web_move .nav .tap_menu li.tap2 a:hover, .web_move .nav .tap_menu li.tap2 a:focus, .web_move .nav .tap_menu li.tap2 a:checked, .web_move .nav .tap_menu li.tap2.on a{background-color:#9080a1; color:#fff;}
.web_move .nav .tap_menu li.tap3 a:hover, .web_move .nav .tap_menu li.tap3 a:focus, .web_move .nav .tap_menu li.tap3 a:checked, .web_move .nav .tap_menu li.tap3.on a{background-color:#a17059; color:#fff;}
.web_move .nav .tap_menu li.tap4 a:hover, .web_move .nav .tap_menu li.tap4 a:focus, .web_move .nav .tap_menu li.tap4 a:checked, .web_move .nav .tap_menu li.tap4.on a{background-color:#6b90bf; color:#fff;}
.web_move .nav .tap_menu li.tap5 a:hover, .web_move .nav .tap_menu li.tap5 a:focus, .web_move .nav .tap_menu li.tap5 a:checked, .web_move .nav .tap_menu li.tap5.on a{background-color:#eb6a5c; color:#fff;}
.web_move .nav .tap_menu li.tap6 a:hover, .web_move .nav .tap_menu li.tap6 a:focus, .web_move .nav .tap_menu li.tap6 a:checked, .web_move .nav .tap_menu li.tap6.on a{background-color:#acac67; color:#fff;}

.web_move .nav .tap_menu li a span.img_area {display:none;}
.web_move .nav .tap_menu li a span.text {margin-top:0; color:#5c5150;}
.web_move .nav .tap_menu li.on a span.text, .web_move .nav .tap_menu li a:hover span.text {color:#fff !important;}
.web_move .foot_btn {position: fixed;right: 80px;bottom: 35px; cursor: pointer;}

.rowgroup {min-width:1200px;}
.rowgroup .visual_bg {height:1100px; background:url('/common/jsp/cj_holiday/images/main/visual_bg.jpg') no-repeat center top; width:100%; text-align:center;}
.rowgroup .visual_bg:before {display:block; width:100%; height:11px; background:url(/common/jsp/cj_holiday/images/main/visual_bg_top.png) no-repeat center top; content:'';}
.rowgroup .visual_bg .vi_text {position:relative; margin-top:100px; font-size:0; line-height:0; text-align:center;}
.rowgroup .visual_bg .vi_text .period {margin-bottom:15px; padding-top:37px; padding-bottom:7px; background:url(/common/jsp/cj_holiday/images/main/period_bg.png) no-repeat center; color:#000; font-size:17px; text-align:center; line-height:22px;}
.rowgroup .visual_bg .vi_text img {margin-bottom:22px;}
.rowgroup .visual_bg .vi_text .text {color:#fff; font-size:21px; line-height:23px; letter-spacing:-0.025em;}
.rowgroup .cts_border {position:relative; background:url('/common/jsp/cj_holiday/images/main/ctsbox_hr.gif') repeat-x; height:35px; top:10px; margin-top:-10px; z-index:10;}
.rowgroup2 {position:relative; background-color:#fff; }
.rowgroup2 .contents {position:relative; background-color:#fff; width:1000px; padding:50px 20px; overflow-x: hidden;}

